<?xml version="1.0"?>

<PropertyList>

 <path>c172p.ac</path>

 <!-- LOD for the model as a whole -->

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>10000</max-m>
 </animation>

 <offsets>
  <z-m>-0.8</z-m>
 </offsets>

 <animation>
  <type>rotate</type>
  <object-name>Elevators</object-name>
  <property>/surface-positions/elevator-pos-norm</property>
  <interpolation>
   <entry>
    <ind>-1.0</ind>
    <dep>-28</dep>
   </entry>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>23</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>5.65</x-m>
   <y-m>0.0</y-m>
   <z-m>0.75</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Elevators</object-name>
  <property>/surface-positions/elevator-pos-norm</property>
  <interpolation>
   <entry>
    <ind>-1.0</ind>
    <dep>-28</dep>
   </entry>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>23</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>5.65</x-m>
   <y-m>0.0</y-m>
   <z-m>0.75</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Rudder</object-name>
  <property>/surface-positions/rudder-pos-norm</property>
  <factor>17.5</factor>
  <center>
   <x-m>5.85</x-m>
   <y-m>0.0</y-m>
   <z-m>1.3</z-m>
  </center>
  <axis>
   <x>0.25</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

</PropertyList>