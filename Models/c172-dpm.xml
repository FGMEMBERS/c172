<?xml version="1.0"?>

<PropertyList>

 <path>c172-dpm.ac</path>

 <offsets>
  <heading-deg>270</heading-deg>
  <z-m>-1</z-m>
 </offsets>

 <!--
   Note that the x and y axes are switched in the following, because
   of the different model orientation.
 -->

 <animation>
  <type>spin</type>
  <object-name>Propeller</object-name>
  <property>/engines/engine[0]/rpm</property>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>.75</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

</PropertyList>